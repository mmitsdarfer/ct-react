{
  "name": "crunchtime",
  "version": "0.1.0",
  "engines": {
    "npm": "9.2.0",
    "node": "18.12.1"
  },
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "concurrently": "^8.2.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "express-async-errors": "^3.1.1",
    "fs": "^0.0.1-security",
    "node-fetch": "^3.3.2",
    "nodemon": "^3.1.4",
    "nth-check": "^2.1.1",
    "puppeteer": "^22.9.0",
    "react": "^18.2.0",
    "react-cookie": "^7.1.4",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.3",
    "react-scripts": "^5.0.1",
    "realm-web": "^2.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "client": "react-scripts start",
    "server": "cd src/backend && nodemon server.js",
    "db": "cd src/backend/mongo && nodemon index.js",
    "start": "concurrently --names \"CLIENT,SERVER,DB\" -c \"bgBlue.bold,bgGreen.bold,bgWhite.bold\" \"npm run client\" \"npm run server\" \"npm run db\" ",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "proxy": "http://localhost:8000/",
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "browser": {
    "fs": false,
    "path": false
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  },
  "description": "This project was bootstrapped with [Create React App](https://github.com/facebook/create-react-app).",
  "main": "./src/React/App.js",
  "author": "",
  "license": "ISC"
}
